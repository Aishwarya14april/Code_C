#include<stdio.h>
int main()
{
    int n,m,size;
    scanf("%d",&n);
    int ar[n];
    for(int i=0;i<n;i++)
    scanf("%d",&ar[i]);
    scanf("%d",&m);
    int st[m];
    for(int i=0;i<m;i++)
    scanf("%d",&st[i]);
    if(n>m)size=n;
    else size=m;
    int temp[size];
    for(int i=0;i<n;i++)
    {
        temp[i]=ar[i]+st[i];
    }
    for(int i=0;i<n;i++)
    {
        printf("%dx^%d ",temp[i],i);
    }
    printf("\n");
    for(int i=0;i<n;i++)
    {
        temp[i]=ar[i]-st[i];
    }
    for(int i=0;i<n;i++)
    {
        printf("%dx^%d ",temp[i],i);
    }
    printf("\n");
    int pro[n+m-1];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            pro[i+j]=0;
        }
    }int ctr;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            ctr=i+j;
            pro[ctr]=pro[ctr]+ar[i]*st[j];
            
        }
    }
    for(int i=0;i<ctr;i++)
    {
            printf("%dx^%d ",pro[i],i);
    }
    return 0;
}
