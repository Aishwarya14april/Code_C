#include<stdio.h>
int main()
{
    int x,y,n,r,c,val;
    scanf("%d %d %d",&x,&y,&n);
    int ar[x][y];
    int temp[x][y];
    for(int i=0;i<x;i++)
    {
        for(int j=0;j<y;j++)
        {
        ar[i][j]=0;
        }
    }
    for(int i=0;i<n;i++)
    {
        scanf("%d %d",&r,&c);
        scanf("%d",&val);
        ar[r][c]=val;
    }
    printf("Array before transpose:\n");
    for(int i=0;i<x;i++)
    {
        for(int j=0;j<y;j++)
        {
        printf("%d ",ar[i][j]);
        }
        printf("\n");
    }
    printf("Array after transpose:\n");
    for(int i=0;i<x;i++)
    {
        for(int j=0;j<y;j++)
        {
            temp[j][i]=ar[i][j];
        }
    }
    for(int i=0;i<y;i++)
    {
        for(int j=0;j<x;j++)
        {
            printf("%d ",temp[i][j]);
        }
        printf("\n");
    }
    return 0;
}
